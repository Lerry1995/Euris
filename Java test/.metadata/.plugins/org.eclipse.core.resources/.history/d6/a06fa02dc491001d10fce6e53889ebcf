package oldFashionPound;

public class Prezzo {
	int p;
	int s;
	int d;
	
	
	public Prezzo(int p, int s, int d) {
		this.p = p;
		this.s = s;
		this.d = d;
	}
	public int getP() {
		return p;
	}
	public void setP(int p) {
		this.p = p;
	}
	public int getS() {
		return s;
	}
	public void setS(int s) {
		this.s = s;
	}
	public int getD() {
		return d;
	}
	public void setD(int d) {
		this.d = d;
	}
	@Override
	public String toString() {
		return "Prezzo : " + p + "p " + s + "s " + d + "d";
	}
	
	
	
	

}
